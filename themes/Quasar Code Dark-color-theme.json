    "tokenColors": [
        // Comments
        {
            "name": "Comments",
            "scope": "comment",
            "settings": { "foreground": "#AAAAAA4D", "fontStyle": "italic" }
        },
        {
            "name": "Comments - Python docstring",
            "scope": "string.quoted.docstring.multi.python",
            "settings": { "foreground": "#AAAAAA4D", "fontStyle": "italic" }
        },
        // Constant
        {
            "name": "Constant - Numeric",
            "scope": "constant.numeric",
            "settings": { "foreground": "#F8961E" }
        },
        {
            "name": "Constant - Character",
            "scope": "constant.character",
            "settings": { "foreground": "#F9844A" }
        },
        {
            "name": "Constant - Language",
            "scope": "constant.language",
            "settings": { "foreground": "#f95b4a" }
        },
        // String
        {
            "name": "String - ...",
            "scope": "string",
            "settings": { "foreground": "#F9C74F" }
        },
